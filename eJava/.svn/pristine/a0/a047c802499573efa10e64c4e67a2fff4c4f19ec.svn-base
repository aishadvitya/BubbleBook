<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"      
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:bb="http://java.sun.com/jsf/composite/bubbles"
      xmlns:p="http://primefaces.org/ui">
    <f:view>
        <f:loadBundle basename="com.iss.bubble.resources.messages" var="msg"/>         
        <h:head>
            <f:facet name="fheader">
                <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
                <meta name="viewport" content="width=device-width"/>
                <h:outputStylesheet library="css" name="mainStyle.css" />
                <h:outputStylesheet library="primefaces-redmond" name="theme.css"/>
                <title><ui:insert name="title"> #{msg.title}</ui:insert></title> 
            </f:facet>
            <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
        </h:head>

        <h:body>
            <p:layout fullPage="true"> 

                <ui:insert name="top"/>
                <ui:insert name="center">
                    Bubble App Content
                </ui:insert>
                <ui:insert name="footer">
                    <bb:footer />
                </ui:insert>

            </p:layout>
        </h:body>
    </f:view>
</html>